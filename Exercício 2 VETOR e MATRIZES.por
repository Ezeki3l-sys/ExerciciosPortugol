programa {
  funcao inicio() {
     cadeia alunos[40]
     inteiro c=0, qtd_alunos_na_sala = 0
     escreva ("Quantos alunos você tem na sala? \n")
     leia (qtd_alunos_na_sala)
     faca 
     {
       escreva ("Digite o nome. \n")
       leia(alunos[c])
       c+=1 // c= c +1


     }enquanto (c!=qtd_alunos_na_sala)
  }
}
